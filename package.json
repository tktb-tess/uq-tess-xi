		"@sveltejs/vite-plugin-svelte": "^6.1.2",
		"@tailwindcss/vite": "^4.1.12",
		"@types/jsdom": "^21.1.7",
		"@types/node": "^24.2.1",
		"@types/papaparse": "^5.3.16",
		"@types/turndown": "^5.0.5",
		"eslint": "^9.33.0",
		"eslint-config-prettier": "^10.1.8",
