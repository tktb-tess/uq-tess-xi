		"lint": "eslint . && prettier --check .",
		"format": "prettier --write .",
		"convert": "npx tsx draft/convert.ts",
		"xml-generate": "npx tsx .other-scripts/xml-generate.ts"
	},
	"devDependencies": {
		"@eslint/compat": "^1.2.5",
