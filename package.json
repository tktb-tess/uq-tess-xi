		"@sveltejs/vite-plugin-svelte": "^5.0.0",
		"@tailwindcss/vite": "^4.0.0",
		"@types/jsdom": "^21.1.7",
		"@types/marked": "^5.0.2",
		"@types/node": "^22.13.14",
		"@types/papaparse": "^5.3.15",
		"@types/turndown": "^5.0.5",
		"eslint": "^9.18.0",
